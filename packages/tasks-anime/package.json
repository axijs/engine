{
  "name": "@axi-engine/tasks-anime",
  "version": "0.1.2",
  "description": "An anime.js adapter for @axi-engine/tasks, enabling skippable animation sequences.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/axijs/engine.git",
    "directory": "packages/tasks-anime"
  },
  "keywords": [
    "axi-engine",
    "typescript",
    "gamedev",
    "tasks",
    "anime.js"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "docs": "typedoc src/index.ts --out docs/api --options ../../typedoc.json",
    "test": "echo 'No tests yet for @axi-engine/tasks-anime'"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@axi-engine/tasks": "^0.1.2",
    "animejs": "^4.2.2"
  },
  "peerDependencies": {
    "@axi-engine/tasks": "^0.1.2",
    "animejs": "^4.2.2"
  }
}
